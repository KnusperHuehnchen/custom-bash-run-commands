{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "background": "#21c7a8",
                    "foreground": "#011627",
                    "leading_diamond": "╭─",
                    "properties": {
                        "windows": ""
                    },
                    "style": "diamond",
                    "template": " {{ if eq .UserName \"kali\"}}Kali at {{ else if  .WSL }}WSL at {{.Icon}}{{ else }}{{.Icon}}{{ end }} ",
                    "trailing_diamond": "",
                    "type": "os"
                },
                {
                    "background": "#ef5350",
                    "foreground": "#ffeb95",
                    "powerline_symbol": "",
                    "style": "powerline",
                    "template": "  ",
                    "type": "root"
                },
                {
                    "background": "#82AAFF",
                    "foreground": "#011627",
                    "powerline_symbol": "",
                    "properties": {
                        "folder_icon": " ",
                        "folder_separator_icon": "<#011627></> ",
                        "home_icon": "  ",
                        "style": "agnoster"
                    },
                    "style": "powerline",
                    "template": "{{ path .Path .Location }}",
                    "type": "path"
                },
                {
                    "background": "#addb67",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#e4cf6a{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f78c6c{{ end }}",
                        "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
                        "{{ if gt .Behind 0 }}#c792ea{{ end }}"
                    ],
                    "foreground": "#011627",
                    "powerline_symbol": "",
                    "properties": {
                        "branch_icon": " ",
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "fetch_worktree_count": true
                    },
                    "style": "powerline",
                    "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
                    "type": "git"
                },
                {
                    "background": "#575656",
                    "foreground": "#d6deeb",
                    "leading_diamond": "",
                    "properties": {
                        "style": "austin",
                        "threshold": 0
                    },
                    "style": "diamond",
                    "template": " {{ .FormattedMs }}",
                    "trailing_diamond": "",
                    "type": "executiontime"
                }
            ],
            "type": "prompt"
        },
